{
  "name": "tachyon-drive-blob-storage",
  "version": "1.0.0",
  "description": "Azure Blob storage driver implementation",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts --sourcemap --format cjs,esm --dts --clean -c tsconfig.build.json",
    "validate": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "repository": "github:mharj/tachyon-drive-blob-storage",
  "keywords": [
    "tachyon",
    "storage",
    "driver",
    "azure",
    "blob"
  ],
  "author": "mharj",
  "license": "MIT",
  "devDependencies": {
    "tachyon-drive-node-fs": "workspace:*"
  },
  "peerDependencies": {
    "@avanio/logger-like": ">= 0.0.1",
    "@azure/storage-blob": "^12",
    "@luolapeikko/ts-common": ">= 0.0.1",
    "tachyon-drive": ">=0.12.0"
  }
}
