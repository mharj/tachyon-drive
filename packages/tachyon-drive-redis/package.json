{
  "name": "tachyon-drive-redis",
  "version": "2.0.0",
  "description": "Tachyon drive implementation for Redis",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts --sourcemap --format cjs,esm --dts --clean -c tsconfig.build.json",
    "validate": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "repository": "github:mharj/tachyon-drive-redis",
  "keywords": [
    "redis",
    "storage",
    "driver",
    "nodejs"
  ],
  "author": "mharj",
  "license": "MIT",
  "devDependencies": {
    "tachyon-drive": "workspace:*",
    "tachyon-drive-node-fs": "workspace:*"
  },
  "peerDependencies": {
    "@avanio/logger-like": ">= 0.1.1",
    "@redis/client": "^5.1",
    "tachyon-drive": ">= 2.0.0"
  }
}
