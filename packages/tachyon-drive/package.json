{
  "name": "tachyon-drive",
  "version": "1.0.0",
  "description": "Extendable typescript/javascript storage driver implementation",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts --sourcemap --format cjs,esm --dts --clean -c tsconfig.build.json",
    "validate": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "repository": "github:mharj/tachyon-drive",
  "keywords": [
    "storage",
    "driver"
  ],
  "author": "mharj",
  "license": "MIT",
  "peerDependencies": {
    "@avanio/logger-like": ">= 0.2.10",
    "@luolapeikko/result-option": ">= 1.0.0",
    "@luolapeikko/ts-common": ">= 0.1.0"
  }
}
