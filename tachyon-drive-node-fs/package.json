		"@types/sinon": "^17.0.4",
		"@vitest/coverage-v8": "^3.2.4",
		"c8": "^10.1.3",
		"oxlint": "^1.2.0",
		"prettier": "^3.5.3",
		"sinon": "^19.0.5",
		"tachyon-drive": "^0.11.5",
		"tslib": "^2.8.1",
		"tsup": "^8.5.0",
