{
	"name": "tachyon-drive",
	"version": "0.3.4",
	"description": "Extendable typescript/javascript storage driver implementation",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"doc": "typedoc",
		"build": "tsup src/index.ts --sourcemap --format cjs,esm --dts --clean",
		"prepublishOnly": "npm run build",
		"test": "nyc mocha",
		"coverage": "nyc report --reporter=lcovonly",
		"lint": "eslint src"
	},
	"files": [
		"dist"
	],
	"repository": "github:mharj/tachyon-drive",
	"keywords": [
		"storage",
		"driver"
	],
	"mocha": {
		"exit": true,
		"extension": [
			"ts",
			"js"
		],
		"recursive": true,
		"require": [
			"ts-node/register",
			"source-map-support/register"
		],
		"reporters": [
			"spec",
			"mocha-junit-reporter"
		]
	},
	"nyc": {
		"check-coverage": true,
		"extension": [
			".ts",
			".js"
		]
	},
	"author": "mharj",
	"license": "MIT",
	"devDependencies": {
		"@tsconfig/node16": "^16.1.1",
		"@types/chai": "^4.3.11",
		"@types/chai-as-promised": "^7.1.8",
		"@types/mocha": "^10.0.6",
		"@types/node": "^18.19.10",
		"@types/sinon": "^17.0.3",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"chai": "^4.4.1",
		"chai-as-promised": "^7.1.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-standard": "^17.1.0",
		"eslint-plugin-deprecation": "^2.0.0",
		"eslint-plugin-jsdoc": "^48.0.4",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-sonarjs": "^0.23.0",
		"mocha": "^10.2.0",
		"nyc": "^15.1.0",
		"prettier": "^3.2.4",
		"sinon": "^17.0.1",
		"source-map-support": "^0.5.21",
		"ts-node": "^10.9.2",
		"tsup": "^8.0.1",
		"typedoc": "^0.25.7",
		"typescript": "^5.3.3",
		"zod": "^3.22.4"
	},
	"dependencies": {
		"@avanio/logger-like": "^0.1.1",
		"@luolapeikko/result-option": "^0.5.3",
		"tslib": "^2.6.2"
	}
}
